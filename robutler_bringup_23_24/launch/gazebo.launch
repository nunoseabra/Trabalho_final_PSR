<?xml version="1.0"?>
<launch>

    <!--Export models to gazebo-->
    <env name="GAZEBO_MODEL_PATH" value="$(find robutler_description_23_24)/models"/>

    <!--Launch of gazebo and options-->
    <include file="$(find gazebo_ros)/launch/empty_world.launch">
        <arg name="world_name" value="$(find robutler_bringup_23_24)/worlds/small_house.world"/>
        <arg name="paused" value="false"/>
        <arg name="use_sim_time" value="true"/>
        <arg name="gui" value="true"/>
        <arg name="headless" value="false"/>
        <arg name="debug" value="false"/>
        
    </include>
</launch>